<openerp>
    <data>

        <!-- 
            More for the picking's form 
        -->
        <record model="ir.ui.view" id="onsp_view_picking_form">
            <field name="name">Open-Net productivity: analytical informations for the pickings</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="ons_cust_solstis.onsp_solstis_stock_picking_view"/>
            <field name="priority" eval="18"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/group[1]/group[1]/field[@name='analytic_cost_id']" position="after">
                    <field name="cost_imputed"/>
                </xpath>
            </field>
        </record>
        
    </data>
</openerp>
