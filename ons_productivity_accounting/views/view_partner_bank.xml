<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="onsp_view_partner_bank_tree">
            <field name="name">Open Net productivity acocunting: partner bank tree</field>
            <field name="model">res.partner.bank</field>
            <field name="inherit_id" ref="base.view_partner_bank_tree"/>
            <field name="priority" eval="17"/>
            <field name="arch" type="xml">
                <field name="sequence" position="attributes">
                    <attribute name="invisible">0</attribute>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="onsp_view_partner_bank_form">
            <field name="name">Open Net productivity acocunting: partner bank from</field>
            <field name="model">res.partner.bank</field>
            <field name="inherit_id" ref="base.view_partner_bank_form"/>
            <field name="priority" eval="17"/>
            <field name="arch" type="xml">
                <group position="inside">
                    <field name="sequence"/>
                </group>
            </field>
        </record>
    </data>
</odoo>