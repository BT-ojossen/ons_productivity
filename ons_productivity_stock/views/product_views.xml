<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="onsp_stock_product_form">
        <field name="name">onsp.stock.product.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="delivery.product_template_hs_code"/>
        <field name="arch" type="xml">
            <field name="hs_code" position="after">
                <field name="onsp_eccn"/>
                <field name="onsp_country_origin"/>
            </field>
        </field>
    </record>

    <record model="ir.ui.view" id="onsp_stock_product_cat_form">
        <field name="name">onsp.stock.product.category.form</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="stock.product_category_form_view_inherit"/>
        <field name="arch" type="xml">
            <field name="removal_strategy_id" position="after">
                <field name="onsp_hs_code"/>
                <field name="onsp_eccn"/>
                <field name="onsp_country_origin"/>
            </field>
        </field>
    </record>
</odoo>